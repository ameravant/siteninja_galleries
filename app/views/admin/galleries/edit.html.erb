<%- content_for :meta_title do -%>Settings for <%= h @gallery.name %><%- end -%>

<h1><%= yield(:meta_title) %></h1>

<% form_for [:admin, @gallery], :html => { :method => :put, :multipart => true, :onsubmit => "form_submitted()" } do |f| %>
<%= render :partial => "form", :locals => { :f => f } %>
<% end %>
